@model Temiskaming.Models.chatSendModel
@{
    Layout = null;
}

@using (Ajax.BeginForm("Send", "Chat", new { file = Session["log"] }, new AjaxOptions
{
    HttpMethod = "Post",
    OnSuccess = "$('#message').val('')"

}, new { @id = "fMessage" }))
{
    @Html.TextBox("message")
    <input type="submit" id="send" name="send" value="Send" />
}


