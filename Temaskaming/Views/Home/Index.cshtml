@model Temiskaming.Models.news
@{
    ViewBag.Title = "Temiskaming Hospital";
}

<article id="home">

    <div class="imgSlider">
        <img src="~/Content/Images/slider/temis3.jpg" />
        <img src="~/content/images/slider/temis1.jpg" />
        <img src="~/Content/Images/slider/temis2.jpg" />
    </div>

    <h2>Latest News</h2>

    <h3>@Model.title</h3>
    
    @Html.Raw(Model.content)

</article>

@section scripts{
    @Scripts.Render("~/scripts/imgSlider.js")
}
